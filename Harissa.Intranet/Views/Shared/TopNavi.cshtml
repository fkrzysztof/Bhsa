<div class="row">
    <div class="col text-left">
        @*<h4><i class="bi bi-chat-left-text m-2" style="font-size: 2.5rem;"></i>@ViewBag.Path</h4>*@
        <h4><i class="@ViewBag.Icon m-2" style="font-size: 2.5rem;"></i> @ViewBag.Path</h4>
    </div>
    <div class="col text-right">
        <a asp-action="Index"><i class="bi bi-arrow-left-circle" style="font-size:2rem; color:black;"></i></a>
    </div>
    <div class="col-12">
        @*<h6>Are you sure you want to delete this?</h6>*@
        <h6>@ViewBag.Message</h6>
        <hr />
    </div>
</div>
